#include<stdio.h>
int main()
{
    int i,m,n,j;
    printf("enter the number of rows   ");
    scanf("%d",&m);
    printf("enter the number of column  ");
    scanf("%d",&n);
    int arr[m][n];
    for (i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {
            printf("A[%d][%d]=",i+1,j+1);
            scanf("%d",&arr[i][j]);
        }
            
    }
    for (i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {
            printf("%d      ",arr[i][j]);
        }
        printf("\n");
        printf("\n");
    }
    int max=-1;
    for (i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {
            if(arr[i][j]>max)
            {
                max=arr[i][j];
            }
        }
    }
        int min=arr[0][0];
    for (i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {
            if(arr[i][j]<min)
            {
                min=arr[i][j];
            }
        }
    }
    printf("max=%d",max);
    printf("min=%d",min);

}
