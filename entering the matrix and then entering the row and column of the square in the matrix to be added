#include<stdio.h>
int main()
{
    int i,m,n,j,a,b;
    printf("enter the number of rows   ");
    scanf("%d",&m);
    printf("enter the number of column  ");
    scanf("%d",&n);
    int arr[m][n];
    for (i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {
            printf("A[%d][%d]=",i+1,j+1);
            scanf("%d",&arr[i][j]);
        }
            
    }
    for (i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {
            printf("%d      ",arr[i][j]);
        }
        printf("\n");
        printf("\n");
    }
    printf("enter the row num of square to be added:  ");
    scanf("%d",&a);
    printf("enter the col num of square to be added:  ");
    scanf("%d",&b);
    for (i=0;i<a;i++)
    {
        for(j=0;j<b;j++)
        {
            printf("%d      ",arr[i][j]);
        }
        printf("\n");
        printf("\n");
    }
    int sum=0;
    for (i=0;i<a;i++)
    {
        for(j=0;j<b;j++)
        {
            sum=sum+arr[i][j];
        }
 
    }
       printf("%d",sum);

}
