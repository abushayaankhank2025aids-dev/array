#include<stdio.h>
int main()
{
    int n,i;
    scanf("%d",&n);
    int arr[n];
    for(i=0;i<n;i++)
    {
        printf("enter element %d",i+1);
        scanf("%d",&arr[i]);
    }
    int min=arr[0];
    for(i=0;i<n;i++)
    {
        if(min>arr[i])
        min=arr[i];
        
    }
    printf("min: %d",min);
}
