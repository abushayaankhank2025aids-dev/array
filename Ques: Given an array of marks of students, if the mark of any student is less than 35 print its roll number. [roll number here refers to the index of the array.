#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n); // Read the number of students

    int arr[n];
    for(int i = 0; i < n; i++) {
        scanf("%d", &arr[i]); // Read marks of each student
    }

    printf("Students with marks less than 35 are at roll numbers: ");
    for(int j = 0; j < n; j++) {
        if(arr[j] < 35) {
            printf("%d ", j);
        }
    }
    printf("\n"); // Add newline at the end
    return 0;
}
