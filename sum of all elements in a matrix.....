#include<stdio.h>
int main()
{
    int i,m,n,j;
    printf("enter the number of rows   ");
    scanf("%d",&m);
    printf("enter the number of column  ");
    scanf("%d",&n);
    int arr[m][n];
    int sum=0;
    for (i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {
            printf("A[%d][%d]=",i+1,j+1);
            scanf("%d",&arr[i][j]);
        }
            printf("\n");
    }

    for (i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {
            sum=sum+arr[i][j];
        }
 
    }
       printf("%d",sum);
}
